<script>
function showHeaderMenu(nm) {
  var d = document.getElementById(nm);
  if (d.style.display != "inline") d.style.display = "inline";
  else d.style.display = "none";
  return false;
}

function hidebanner() {
  document.cookie="nov6ok=-1"; 
  document.getElementById("banner").innerHTML="";
}

var g_onload = function() {

  var t = document.getElementById("banner");
  if (t && (new Date) < (new Date(2016,10,8,19,0,0))) { 
    var s = '';
//     s += '<div style="float: right; background: #ffd; margin-top:12px; margin-right:8px; padding: 8px; padding-left: 11px; padding-right: 11px;" onclick="hidebanner();">X</div>';
   s+= '<div width=100% style="blackground:black; padding:3px;"></div>';
     s += '<div width=100% style="background: white; text-align: center; padding:5px; font-size:20px;">PLEASE VOTE!</div><div style="background: white; padding-bottom: 7px; text-align:center; font-size:12px;">';
    var urls =  [
"http://www.nytimes.com/2016/09/25/opinion/sunday/hillary-clinton-for-president.html",
"http://www.theatlantic.com/magazine/archive/2016/11/the-case-for-hillary-clinton-and-against-donald-trump/501161/",
"https://www.washingtonpost.com/opinions/hillary-clinton-for-president/2016/10/12/665f9698-8caf-11e6-bf8a-3d26847eeed4_story.html",
"http://www.usatoday.com/story/opinion/2016/09/29/dont-vote-for-donald-trump-editorial-board-editorials-debates/91295020/",
"https://www.wired.com/2016/08/wired-endorses-hillary-clinton/",
"http://www.newyorker.com/magazine/2016/10/31/the-new-yorker-endorses-hillary-clinton",
"https://www.bostonglobe.com/opinion/editorials/2016/10/08/hillary-clinton-for-president/FskxtCff9ErrN3TW4OJb6O/story.html",
"https://www.scientificamerican.com/article/donald-trump-s-lack-of-respect-for-science-is-alarming/",
"http://yalerecord.org/2016/10/26/the-yale-record-does-not-endorse-hillary-clinton/",
    ];
    for (var x in urls) {
      s += '&nbsp;<a href="' + urls[x] + '" target="_blank" rel=nofollow>&bull;</a>';
    }

    s += '</div>';
    t.innerHTML = s;
  }


};
</script>


</head>
